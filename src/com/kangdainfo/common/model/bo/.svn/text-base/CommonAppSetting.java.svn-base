package com.kangdainfo.common.model.bo;

/**
 * 一些啟始就設定的AP參數收集資訊
 * @see WEB-INF/properites/app.properties
 * @author KangDa 
 *
 */
public class CommonAppSetting {
	
	/**
	 * SMB, FTP, WS
	 */
	private String defaultFileUploadHandler;
	
	/**
	 * SMB, FTP, WS
	 * @return
	 */
	public String getDefaultFileUploadHandler() {
		if (defaultFileUploadHandler!=null) return defaultFileUploadHandler;
		else return "SMB";
	}
	
	/**
	 * SMB, FTP, WS
	 * @param defaultFileUploadHandler
	 */
	public void setDefaultFileUploadHandler(String defaultFileUploadHandler) {this.defaultFileUploadHandler = defaultFileUploadHandler;	}

	
}
